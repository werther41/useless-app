    "clsx": "^1.2.1",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.22",
    "lucide-react": "0.105.0-alpha.4",
    "next": "^13.4.8",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "rehype": "^13.0.2",
    "rehype-katex": "^7.0.1",
