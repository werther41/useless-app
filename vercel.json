{
  "functions": {
    "app/api/facts/stats/route.ts": {
      "maxDuration": 10
    },
    "app/api/facts/route.ts": {
      "maxDuration": 10
    },
    "app/api/facts/random/route.ts": {
      "maxDuration": 10
    },
    "app/api/facts/top-rated/route.ts": {
      "maxDuration": 10
    },
    "app/api/facts/bottom-rated/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/facts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0, s-maxage=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
